<Application
	x:Class="Reversi.App"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	StartupUri="Views/GameWindow.xaml">

	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary>
					<ImageBrush x:Key="BoardBackgroundBrush" ImageSource="/Assets/BoardBackground.png" TileMode="Tile" Viewport="0,0,100,100" ViewportUnits="Absolute" />
				</ResourceDictionary>
				<ResourceDictionary Source="Themes/Generic.xaml" />
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Application.Resources>
</Application>
